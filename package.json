{
  "name": "study-claude-code",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:worker": "echo 'Worker build handled by wrangler deploy'",
    "dev": "react-router dev --host",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "server": "tsx watch server.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy:worker": "wrangler deploy",
    "deploy:pages": "npm run build && wrangler pages deploy build/client",
    "deploy": "npm run deploy:worker && npm run deploy:pages"
  },
  "dependencies": {
    "@eslint/js": "^9.28.0",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "hono": "^4.7.11",
    "isbot": "^5.1.27",
    "jsonwebtoken": "^9.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tsx": "^4.19.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250607.0",
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "supertest": "^7.1.1",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.3",
    "wrangler": "^4.19.1"
  }
}
